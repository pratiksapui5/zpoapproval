{
	"_version": "1.12.0",
	"sap.app": {
		"id": "POApproval.ZPOApproval",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZVECV_PURCHASE_ORDER_APPROVAL_SRV": {
				"uri": "/sap/opu/odata/sap/ZVECV_PURCHASE_ORDER_APPROVAL_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "POApproval.ZPOApproval.view.SApp",
			"type": "XML",
			"async": true,
			"id": "idApp"
		},
		"dependencies": {
			"minUI5Version": "1.38.19",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "POApproval.ZPOApproval.i18n.i18n"
				}
			},
			"PurchaseOrderSet": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "jsondata/PurchaseOrderData.json",
				"preload": false
			},
			"AttachmentItemsSet": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "jsondata/AttachmentItems.json",
				"preload": false
			},
			"UserDataSet": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "jsondata/ToUser.json",
				"preload": false
			},
			"QueryHistorySet": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "jsondata/QueryHistory.json"
			},
			"ApprovalHistorySet": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "jsondata/ApprovalHistory.json"
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZVECV_PURCHASE_ORDER_APPROVAL_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "POApproval.ZPOApproval.view",
				"controlAggregation": "pages",
				"controlId": "idApp",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"pattern": "",
					"name": "FirstPage",
					"target": [
						"POApprovalDetail",
						"POApprovalMaster"
					]
				},
				{
					"name": "POApprovalDetail",
					"pattern": "POApprovalDetail/{PO_No}",
					"target": [
						"POApprovalMaster",
						"POApprovalDetail"
					]
				},
				{
					"name": "POApprovalMaster",
					"pattern": "POApprovalMaster",
					"target": [
						"POApprovalMaster",
						"POApprovalDetail"
					]
				},
				{
					"name": "RouteSApp",
					"pattern": "RouteSApp",
					"target": [
						"TargetSApp"
					]
				}
			],
			"targets": {
				"POApprovalMaster": {
					"viewName": "POApprovalMaster",
					"controlAggregation": "masterPages"
				},
				"POApprovalDetail": {
					"viewName": "POApprovalDetail",
					"controlAggregation": "detailPages"
				},
				"TargetSApp": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "idApp",
					"viewName": "SApp"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zpoapproval/webapp",
		"_version": "1.1.0"
	}
}